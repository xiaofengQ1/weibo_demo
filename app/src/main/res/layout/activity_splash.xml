<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SplashScreen">
    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/frame2"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="210dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/frame2"
        />
    <TextView
        android:layout_width="58dp"
        android:layout_height="21dp"
        app:layout_constraintTop_toBottomOf="@id/frame2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        android:text="@string/IHWeiBo"
        android:textSize="16dp"
        android:textStyle="bold"
        android:gravity="center"
        android:id="@+id/frame4_1"
        />
    <TextView
        android:layout_width="56dp"
        android:layout_height="16dp"
        android:id="@+id/frame4_2"
        app:layout_constraintTop_toBottomOf="@id/frame4_1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:text="@string/frame4_2"
        android:textSize="12dp"
        android:gravity="center"
        android:textColor="@color/gray"
        />
</androidx.constraintlayout.widget.ConstraintLayout>